{
  "name": "@office-iss/sdx_helloworld",
  "description": "Simple SDX stub implementation",
  "version": "0.0.98",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "npm run start-desktop",
    "start-desktop": "office-addin-debugging start manifest.xml --source-bundle-url-path index.win32 --packager \"npm run packager\"",
    "stop": "npm run disable-debugging",
    "packager": "node ../../scripts/start-native.js",
    "build": "node ../../scripts/build.js",
    "buildfast": "node ../../scripts/buildfast.js",
    "buildfull": "node ../../scripts/buildfull.js",
    "clean": "node ../../scripts/clean.js",
    "watch": "node ../../scripts/watch.js",
    "makebundle": "node ../../scripts/build.js native-bundle win32 dev",
    "sideload": "node node_modules/office-toolbox/app/office-toolbox.js sideload -m manifest.xml -a Excel",
    "sideload-apple": "node node_modules/office-toolbox/app/office-toolbox.js sideload -m manifest_apple.xml -a Excel",
    "prepackage": "npm run clean && npm run build",
    "validate-manifest": "node node_modules/office-toolbox/app/office-toolbox.js validate -m manifest.xml",
    "package": "node node_modules/@ms/office-build-tools/lib/OfficeBuildTools.js packageAddin -m manifest.xml -i dist\\win32\\ship src\\assets -p dist\\sdx_helloworld",
    "packageAddin": "node node_modules/@ms/office-build-tools/lib/OfficeBuildTools.js packageAddin -w lib\\temp -i dist\\win32\\ship src\\assets -p lib\\sdx_helloworld",
    "disable-debugging": "office-addin-dev-settings disable-debugging manifest.xml"
  },
  "dependencies": {
    "@microsoft/office-api": "0.1.0-beta.10",
    "@office-iss/polyester": "1.87.24",
    "@office-iss/reka-react-native": "0.8.62",
    "prop-types": "15.6.2",
    "react": "16.3.1"
  },
  "devDependencies": {
    "@ms/office-build-tools": "^0.0.4",
    "@types/es6-collections": "^0.5.29",
    "@types/es6-promise": "0.0.32",
    "@types/react": "15.0.38",
    "@types/react-dom": "^15.5.0",
    "@types/react-native": "~0.55.1",
    "@office-iss/react-native-win32": "0.8.38",
    "@office-iss/sdx-platform-tslint": "1.0.4",
    "@office-iss/sdx-build-tools": "0.3.0",
    "@office-iss/react-native": "0.3010.2",
    "react-dom": "16.3.1",
    "office-toolbox": "^0.1.0",
    "office-addin-node-debugger": "^0.3.0",
    "office-addin-debugging": "^0.0.1",
    "office-addin-dev-settings": "^1.0.0"
  },
  "disabledTasks": [
    "copy",
    "jest",
    "sass"
  ],
  "nativeBundles": {
    "targets": [
      "win32.ship",
      "win32.dev"
    ]
  },
  "appx": {
    "win32": {
      "operation": "packageAddin",
      "assetid": "HelloWorld",
      "manifest": "manifest.xml",
      "include": [
        "dist\\win32\\ship\\index.win32.bundle",
        "assets"
      ],
      "package": "dist\\en-us_win32"
    },
    "web": {
      "operation": "packageAddin",
      "assetid": "HelloWorld",
      "manifest": "manifest_web.xml",
      "include": [
        "lib\\en-us_web\\bundle.js",
        "src\\index.html"
      ],
      "package": "dist\\en-us_web"
    }
  }
}